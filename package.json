{
  "name": "frixxer",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.7.0-alpha",
    "react-dom": "^16.7.0-alpha",
    "react-motion": "^0.5.2",
    "react-redux": "^6.0.0",
    "react-scripts": "2.1.1",
    "redux-starter-kit": "^0.3.0"
  },
  "scripts": {
    "build:css": "postcss src/index.css -o src/tmp/index.css",
    "watch:css": "postcss src/index.css -o src/tmp/index.css -w",
    "start:rs": "react-scripts start",
    "build:rs": "react-scripts build",
    "start": "run-p watch:css start:rs",
    "build": "run-s build:css build:rs",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/jest": "23.3.10",
    "@types/node": "10.12.12",
    "@types/react": "16.7.13",
    "@types/react-dom": "16.0.11",
    "@types/react-motion": "0.0.27",
    "@types/react-redux": "^6.0.12",
    "happy-helpers": "^1.4.0",
    "husky": "^1.2.0",
    "jest-dom": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.0.1",
    "prettier": "1.15.3",
    "pretty-quick": "^1.8.0",
    "react-testing-library": "^5.3.2",
    "tailwindcss": "^0.7.2",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "3.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
